[push]
	default = simple

[alias]
  co = checkout
  ci = commit
  st = status
  br = branch
  hist = log --pretty=format:\"%h %ad | %s%d [%an]\" --graph --date=short
  type = cat-file -t
  dump = cat-file -p

[merge]
	tool = vimdiff
[diff]
	tool = vimdiff
[difftool]
	prompt = false
[core]
	editor = vim
